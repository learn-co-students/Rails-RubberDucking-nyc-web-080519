<h1>Student Show Page</h1>

<h3>Name: <%= @student.name %></h3>

<h3>Mod #: <%= @student.mod %></h3>

<ul>
    <% @student.ducks.each do |duck| %>
        <li><%= link_to duck.name, duck_path(duck.id) %></li>
    <% end %>
</ul>


<%= button_to "delete", student_path(@student.id), :method => :delete %>